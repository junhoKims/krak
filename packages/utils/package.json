{
  "name": "@krak/utils",
  "version": "0.0.1",
  "description": "krak utils",
  "author": "junhoKims",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./commons": {
      "require": "./dist/commons/index.cjs",
      "import": "./dist/commons/index.mjs",
      "default": "./dist/commons/index.mjs",
      "types": "./dist/commons/index.d.ts"
    },
    "./react": {
      "require": "./dist/react/index.cjs",
      "import": "./dist/react/index.mjs",
      "default": "./dist/react/index.mjs",
      "types": "./dist/react/index.d.ts"
    },
    "./*": {
      "require": "./dist/*/index.cjs",
      "import": "./dist/*/index.mjs",
      "default": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "scripts": {
    "check:lint": "eslint --fix --cache .",
    "check:type": "tsc -p tsconfig.json --noEmit --incremental",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --silent",
    "test:ui": "vitest --ui",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && rollup -c rollup.config.js --silent"
  },
  "dependencies": {
    "dayjs": "1.11.13"
  },
  "devDependencies": {
    "@krak/eslint-config": "workspace:^",
    "@krak/prettier-config": "workspace:^",
    "@krak/rollup-config": "workspace:^",
    "@krak/typescript-config": "workspace:*",
    "@krak/vitest-config": "workspace:^",
    "@types/global-types": "workspace:^",
    "@types/node": "22.10.2",
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
