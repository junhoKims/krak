{
  "name": "@krak/ui",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./base": {
      "require": "./dist/core/index.cjs",
      "import": "./dist/core/index.mjs",
      "default": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    },
    "./core": {
      "require": "./dist/core/index.cjs",
      "import": "./dist/core/index.mjs",
      "default": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    },
    "./*": {
      "require": "./dist/*/index.cjs",
      "import": "./dist/*/index.mjs",
      "default": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    },
    "./styles-ui.css": "./dist/styles/styles-ui.css"
  },
  "scripts": {
    "check:lint": "eslint --fix --cache .",
    "check:type": "tsc -p tsconfig.json --noEmit --incremental",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && rollup -c rollup.config.js --silent"
  },
  "devDependencies": {
    "@krak/eslint-config": "workspace:*",
    "@krak/prettier-config": "workspace:^",
    "@krak/rollup-config": "workspace:^",
    "@krak/typescript-config": "workspace:*",
    "@types/global-types": "workspace:^",
    "@types/node": "22.10.2",
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
